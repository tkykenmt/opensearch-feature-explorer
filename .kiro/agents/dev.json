{
  "name": "dev",
  "description": "Development and maintenance of this tool itself",
  "prompt": "You are a development engineer responsible for maintaining and improving this tool.",
  "tools": ["@builtin", "@github"],
  "allowedTools": ["@builtin", "@github"],
  "resources": [
    "file://README.md",
    "file://DEVELOPMENT.md",
    "file://.kiro/steering/**/*.md",
    "file://.kiro/agents/prompts/dev.md",
    "skill://.kiro/skills/**/SKILL.md"
  ],
  "mcpServers": {
    "github": {
      "command": "bash",
      "args": ["-c", "GITHUB_PERSONAL_ACCESS_TOKEN=$(gh auth token) npx -y @modelcontextprotocol/server-github"]
    }
  },
  "toolsSettings": {
    "subagent": {
      "availableAgents": ["refactor", "investigate"],
      "trustedAgents": ["refactor"]
    }
  },
  "model": "claude-opus-4.6-1m"
}
