{
  "name": "fetch-release",
  "description": "Fetch release notes and save all items to JSON cache",
  "prompt": ".kiro/agents/prompts/fetch-release.md",
  "tools": ["@builtin", "@github"],
  "allowedTools": ["@builtin", "@github"],
  "mcpServers": {
    "github": {
      "command": "bash",
      "args": ["-c", "GITHUB_PERSONAL_ACCESS_TOKEN=$(gh auth token) npx -y @modelcontextprotocol/server-github"]
    }
  }
}
